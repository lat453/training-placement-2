class Student:
    def __init__(self, name, marks):
        self.name = name
        self.marks = marks

    def average(self):
        return sum(self.marks) / len(self.marks)

    def display(self):
        print(f"Name: {self.name}")
        print("Marks:", self.marks)
        print("Average:", self.average())

s1 = Student("Latchana", [85, 90, 78])
s1.display()
